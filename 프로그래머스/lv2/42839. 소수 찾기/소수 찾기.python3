from itertools import permutations

def isPrime(n):
    if n < 2:
        return False
    for i in range(2, int(n**0.5)+1):
        if n%i == 0:
            return False
    return True

def solution(numbers):
    check = []
    numbers = list(numbers)
    ans = []

    for i in range(1, len(numbers)+1):
        check += list(set((permutations(numbers, i))))
    check = set(map(int, [''.join(i) for i in check]))

    
    return sum([1 for i in check if isPrime(i)])
        